package variables;

public class ManejoCadenasBasico {
	public static void main(String args []) {
		String nombre='Enrique';
		String apellido='Zetina';
		
		Sytem.out.println("Concatenacion: " + nombre + apellido);
		Sytem.out.println("Nueva Linea: \n" + nombre + " " + apellido);
		Sytem.out.println("Tabulador: \t" + nombre + " " + apellido);
		
	}

}
